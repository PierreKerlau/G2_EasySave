<Window x:Class="Livrable1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Livrable1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid Margin="0,0,0,346">
            <Label x:Name="LabelMainViewTitle" Content="EasySave" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Franklin Gothic Demi" FontSize="36"/>
        </Grid>
        <Grid Margin="0,93,0,0">
            <Button x:Name="ButtonShowViewAddBackup" Content="Add a backup job" HorizontalAlignment="Center" Margin="0,58,0,0" VerticalAlignment="Top" Click="ButtonShowViewAddBackup_Click"/>
            <Button x:Name="ButtonShowViewParameter" Content="Modify parameters" HorizontalAlignment="Left" Margin="648,276,0,0" VerticalAlignment="Top" Click="ButtonShowViewParameter_Click"/>
            <Button x:Name="ButtonShowViewExecuteBackup" Content="Execute a backup job" HorizontalAlignment="Center" Margin="0,113,0,0" VerticalAlignment="Top" Click="ButtonShowViewExecuteBackup_Click"/>
            <Button x:Name="ButtonShowViewRecoverBackup" Content="Recover a backup" HorizontalAlignment="Center" Margin="0,168,0,0" VerticalAlignment="Top" Click="ButtonShowViewRecoverBackup_Click"/>
        </Grid>

    </Grid>
</Window>
